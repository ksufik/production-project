  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev:server": "node ./json-server/index.js",
    "json-server": "npx json-server ./json-server/db.json --port 8000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "npx eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "test:jest": "jest --config ./config/jest/jest.config.ts",
    "test:loki": "npx loki test",
    "test:loki:ok": "npx loki approve",
    "test:loki:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:loki:json": "node scripts/generate-visual-json-report.js",
    "test:loki:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:loki:report": "npm run test:loki:json && npm run test:loki:html",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook --test",
    "prepare": "husky install"
  },
